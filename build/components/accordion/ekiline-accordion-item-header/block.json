{
  "apiVersion": 3,
  "name": "ekiline-block-collection/ekiline-accordion-item-header",
  "title": "Accordion item header",
  "category": "design",
  "icon": "button",
  "description": "Set title and content in your accordion container.",
  "parent": [
    "ekiline-block-collection/ekiline-accordion-item"
  ],
  "usesContext": [
    "ekiline-accordion-item/anchor",
    "ekiline-accordion-item/itemTarget"
  ],
  "supports": {
    "html": false,
    "reusable": false,
    "inserter": false,
    "color": {
      "gradients": true
    }
  },
  "attributes": {
    "itemTarget": {
      "type": "string",
      "default": ""
    },
    "content": {
      "type": "string",
      "source": "html",
      "selector": "button",
      "default": "Item title."
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css"
}